<?xml version="1.0" encoding="UTF-8"?>
	<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.baidu.firstboot.dao.ILogDAO">
	<insert id="doCreateLog" parameterType="log">
		insert into log(description,logType,requestUrl,requestType,username,ip,costTime,createTime)
		values(#{description},#{logType},#{requestUrl},#{requestType},#{username},#{ip},#{costTime},#{createTime})
	</insert>
</mapper>